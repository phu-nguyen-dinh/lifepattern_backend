{
  "info": {
    "name": "LifePattern AI API",
    "description": "Complete API collection for LifePattern AI Backend",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:8080/api"
    },
    {
      "key": "token",
      "value": ""
    }
  ],
  "item": [
    {
      "name": "Authentication",
      "item": [
        {
          "name": "Register",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"user@example.com\",\n  \"password\": \"password123\",\n  \"name\": \"John Doe\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/auth/register",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "register"]
            }
          }
        },
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"user@example.com\",\n  \"password\": \"password123\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/auth/login",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "login"]
            }
          }
        },
        {
          "name": "Get Current User",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/auth/me",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "me"]
            }
          }
        },
        {
          "name": "Forgot Password",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"user@example.com\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/auth/forgot-password",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "forgot-password"]
            }
          }
        }
      ]
    },
    {
      "name": "Daily Logs",
      "item": [
        {
          "name": "Get All Logs",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/logs",
              "host": ["{{baseUrl}}"],
              "path": ["logs"]
            }
          }
        },
        {
          "name": "Create Log",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"date\": \"2024-02-21\",\n  \"sleepHours\": 7.5,\n  \"workHours\": 8.0,\n  \"studyHours\": 2.0,\n  \"entertainmentHours\": 3.0,\n  \"energyLevel\": 7,\n  \"stressLevel\": 5,\n  \"notes\": \"Productive day\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/logs",
              "host": ["{{baseUrl}}"],
              "path": ["logs"]
            }
          }
        },
        {
          "name": "Get Log by ID",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/logs/1",
              "host": ["{{baseUrl}}"],
              "path": ["logs", "1"]
            }
          }
        },
        {
          "name": "Delete Log",
          "request": {
            "method": "DELETE",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/logs/1",
              "host": ["{{baseUrl}}"],
              "path": ["logs", "1"]
            }
          }
        }
      ]
    },
    {
      "name": "Analysis",
      "item": [
        {
          "name": "Get Latest Analysis",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/analysis/latest",
              "host": ["{{baseUrl}}"],
              "path": ["analysis", "latest"]
            }
          }
        },
        {
          "name": "Get Trends (by days)",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/analysis/trends?days=7",
              "host": ["{{baseUrl}}"],
              "path": ["analysis", "trends"],
              "query": [
                {
                  "key": "days",
                  "value": "7"
                }
              ]
            }
          }
        },
        {
          "name": "Get Trends (by date range)",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/analysis/trends?start=2024-02-01&end=2024-02-21",
              "host": ["{{baseUrl}}"],
              "path": ["analysis", "trends"],
              "query": [
                {
                  "key": "start",
                  "value": "2024-02-01"
                },
                {
                  "key": "end",
                  "value": "2024-02-21"
                }
              ]
            }
          }
        },
        {
          "name": "Regenerate Analysis",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/analysis/regenerate",
              "host": ["{{baseUrl}}"],
              "path": ["analysis", "regenerate"]
            }
          }
        }
      ]
    }
  ]
}
